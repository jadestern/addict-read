{
	"name": "feedic",
	"private": true,
	"version": "0.0.149",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"dev:netlify": "netlify dev",
		"build": "tsc -b && vite build",
		"preview": "vite preview",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:e2e": "playwright test",
		"test:e2e:fast": "playwright test --max-failures=1 --reporter=line",
		"test:e2e:headed": "playwright test --headed",
		"test:e2e:ui": "playwright test --ui",
		"format": "prettier --check .",
		"format:fix": "prettier --write .",
		"lint": "oxlint",
		"lint:fix": "oxlint --fix",
		"check": "bun run format && bun run lint && bunx tsc --noEmit",
		"check:fix": "bun run format:fix && bun run lint:fix && bunx tsc --noEmit"
	},
	"dependencies": {
		"jazz-tools": "^0.17.5",
		"react": "19.1.0",
		"react-dom": "19.1.0",
		"react-router-dom": "^7.8.1",
		"rss-parser": "^3.13.0"
	},
	"devDependencies": {
		"@playwright/test": "^1.54.2",
		"@tailwindcss/postcss": "^4.1.10",
		"@testing-library/jest-dom": "^6.7.0",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@types/node": "^24.3.0",
		"@types/react": "19.1.0",
		"@types/react-dom": "19.1.0",
		"@vitejs/plugin-react": "^4.5.1",
		"@vitest/ui": "^3.2.4",
		"globals": "^15.11.0",
		"is-ci": "^3.0.1",
		"jsdom": "^26.1.0",
		"netlify-cli": "^23.1.4",
		"oxlint": "^1.12.0",
		"postcss": "^8.4.27",
		"prettier": "^3.6.2",
		"tailwindcss": "^4.1.10",
		"typescript": "5.6.2",
		"vite": "6.3.5",
		"vitest": "^3.2.4"
	}
}
